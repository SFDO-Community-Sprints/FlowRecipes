        </outputAssignments>
        <outputAssignments>
            <assignToReference>ContactID</assignToReference>
            <field>Id</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>FirstGiftDateonContact</assignToReference>
            <field>npo02__FirstCloseDate__c</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>Deceased</assignToReference>
            <field>npsp__Deceased__c</field>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>DoNotContact</assignToReference>
            <field>npsp__Do_Not_Contact__c</field>
        </outputAssignments>
    </recordLookups>
